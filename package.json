            },
            {
                "language": "javascript",
                "path": "typescriptreact.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/harish9312/react-snippets.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "icon.png"
}